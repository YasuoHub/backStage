<script>
    $("#suggestTable").on('click','.replyfail',function (event) {
        var stid=$(event.target).parents('.center').data('stid');
            $.ajax({
                url: 'suggestReply',
                type: 'POST',
                dataType: 'text',
                data: {delateStatus: '0',stid:stid}
            })
                .done(function(data) {
                    if(data=='1'){
                        $(event.target).parents('tr').remove();
                    }else{
                        alert('未知错误，请刷新后再处理！');
                    }
                });
    });
    $("#suggestTable").on('click','.replyok',function (event) {
        $('#returnModal').modal('show');
        var nowstid=$(event.target).parents('.center').data('stid');
        console.log('now STID:'+nowstid);
        console.log($(event.target).parents('.center'));
        $('#suggestApplySubmit').on('click', function submitSuggest() {
            var replyContent=$('#replyContent').val();
            $('#replyContent').val('');
            $.ajax({
                url: 'suggestReply',
                type: 'POST',
                dataType: 'text',
                data: {delateStatus:'1',nowstid:nowstid,replyContent:replyContent}
            })
                .done(function(data) {
                    if(data=='1'){
                        $(event.target).parents('tr').remove();
                        $('#suggestApplySubmit').off('click');
                    }else{
                        alert('未知错误，请刷新后再处理！');
                    }
                });
        })

    });
</script>